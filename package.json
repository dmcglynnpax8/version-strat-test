{
  "name": "version-strat-test",
  "version": "0.0.1",
  "author": "dmg, group:default/usage-and-provisioning",
  "repository": {
    "description": "version-strat-test Webpack Module Federation Microfrontend (MFE) app for dmg's MFE architecture",
    "type": "git",
    "url": "git+https://github.com/dmg/version-strat-test.git"
  },
  "scripts": {
    "build:nonproduction": "vue-cli-service build --mode nonproduction",
    "build:preproduction": "vue-cli-service build --mode preproduction",
    "build:production": "vue-cli-service build --mode production",
    "requirements-check": "node check_node_version.js",
    "vitest": "vitest",
    "vitest:ui": "vitest --ui",
    "vitest:ci": "vitest run",
    "vitest:coverage": "vitest run --coverage",
    "pact": "jest",
    "lint": "vue-cli-service lint --no-fix",
    "lint:ci": "NODE_ENV=production vue-cli-service lint --no-fix",
    "tsc": "vue-tsc --noEmit",
    "prettier": "prettier --config ./.prettierrc.cjs -c .",
    "vue-i18n-extract": "node_modules/vue-i18n-extract/bin/vue-i18n-extract.js report --vueFiles \"./src/**/*.?(vue|js|ts)\" --languageFiles ./largeKvpFile.json",
    "i18n-kvp-test": "node combineJSONKvps.js && npm run vue-i18n-extract",
    "prepare": "husky install",
    "check-for-errors": "concurrently -c 'blue.bold,magenta.bold,yellow.bold' \"npm:lint\" \"npm:tsc\" \"npm:prettier\" \"npm:i18n-kvp-test\""
  },
  "dependencies": {
    "@types/papaparse": "^5.3.15",
    "axios": "^1.6.0",
    "core-js": "^3.35.0",
    "lodash": "4.17.2",
    "papaparse": "^5.4.1",
    "vue": "3.4.10",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@module-federation/fmr": "^0.0.7",
    "@pact-foundation/pact": "^12.1.2",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.14.202",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-istanbul": "^3.0.8",
    "@vitest/ui": "^3.0.8",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.4.10",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "msw": "^2.0.13",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "recursive-readdir": "^2.2.3",
    "semver": "^7.5.2",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "vitest": "^3.0.8",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^1.8.27",
    "webpack-dev-server": "^4.15.1",
    "@rushstack/eslint-patch": "^1.6.1",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.0"
  },
  "engines": {
    "node": "22.15.0"
  }
}
