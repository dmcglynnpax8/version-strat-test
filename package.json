{
  "name": "dependabot-versioning-strategy-test",
  "version": "0.0.1",
  "author": "pax8, group:default/sso-platformsec-dev",
  "repository": {
    "description": "dependabot-versioning-strategy-test Webpack Module Federation Microfrontend (MFE) app for Pax8's MFE architecture",
    "type": "git",
    "url": "git+https://github.com/pax8/canary-mfe.git"
  },
  "scripts": {
    "serve": "node scripts/serve.mjs",
    "build:nonproduction": "vue-cli-service build --mode nonproduction",
    "build:preproduction": "vue-cli-service build --mode preproduction",
    "build:production": "vue-cli-service build --mode production",
    "requirements-check": "node check_node_version.js",
    "vitest": "vitest",
    "vitest:ci": "vitest run",
    "vitest:coverage": "vitest run --coverage",
    "vitest:ui": "vitest --ui",
    "lint": "vue-cli-service lint --no-fix",
    "lint:ci": "NODE_ENV=production vue-cli-service lint --no-fix",
    "tsc": "vue-tsc --noEmit",
    "prettier": "prettier --config ./.prettierrc.cjs -c .",
    "prettier:w": "prettier --write --config ./.prettierrc.cjs -c .",
    "vue-i18n-extract": "node_modules/vue-i18n-extract/bin/vue-i18n-extract.js report --vueFiles \"./src/**/*.?(vue|js|ts)\" --languageFiles ./largeKvpFile.json",
    "i18n-kvp-test": "node combineJSONKvps.js && npm run vue-i18n-extract",
    "prepare": "husky install",
    "check-for-errors": "concurrently -c 'blue.bold,magenta.bold,yellow.bold' \"npm:lint\" \"npm:tsc\" \"npm:prettier\" \"npm:i18n-kvp-test\"",
    "test:pact": "npx playwright test src/tests/contract/ --project=chromium"
  },
  "dependencies": {
    "@pact-foundation/pact": "^13.2.0",
    "@pax8/error-formatter": "^0.0.3",
    "@pax8/observability": "^1.5.0",
    "@pax8/propulsion": "latest",
    "axios": "^1.7.2",
    "lodash": "^4.17.2",
    "vue": "3.5.12",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
    "@babel/preset-typescript": "^7.23.3",
    "@pax8/eslint-plugin-global-fe": "^0.0.1",
    "@pax8/feature-flags": "^2.0.0",
    "@pax8/mfe-core": "^1.5.0",
    "@pax8/p8p-mfe-shell": "latest",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.14.202",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-istanbul": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.4.10",
    "@vue/eslint-config-typescript": "^12.0.0",
    "chalk": "^5.3.0",
    "concurrently": "^9.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "figlet": "^1.7.0",
    "husky": "^8.0.3",
    "inquirer": "^10.1.8",
    "jsdom": "^23.2.0",
    "jwt-decode": "^4.0.0",
    "msw": "^2.4.0",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "recursive-readdir": "^2.2.3",
    "semver": "^7.6.3",
    "shelljs": "^0.8.5",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "vitest": "^2.1.5",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": "20"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
